{
  "dependencies": {
    "weixin-js-sdk": "^1.6.0"
  },
  "devDependencies": {
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^9.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.2"
  },
  "scripts": {
    "prettier": "npx prettier --write ."
  },
  "pre-commit": [
    "prettier"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
   "src/**/*.{js,vue}": [
    "eslint --fix",
    "prettier --write"
   ]
  }
}
